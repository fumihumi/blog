{
  "name": "blog.fumihumi.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "NODE_OPTIONS='--inspect' next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "build": "pathpida --enableStatic && next build",
    "lint": "eslint components/**/*.{ts,tsx} lib/**/*.ts pages/*.tsx",
    "updates:npm-packages": "git checkout -b ncu_`date +%Y%m%d` && ncu -i"
  },
  "dependencies": {
    "github-markdown-css": "^4.0.0",
    "gray-matter": "^4.0.2",
    "next": "^10.2.2",
    "raw-loader": "^4.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.10.2",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  }
}
